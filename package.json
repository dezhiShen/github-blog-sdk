        "axios": "^0.21.4",
        "core-js": "^3.17.3",
        "marked": "^3.0.4"
    },
    "browser": {
        "crypto": false
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/polyfill": "^7.12.1",
